                        &lt;L7p:entry&gt;
                            &lt;L7p:key stringValue="givenErrorCode"/&gt;
                            &lt;L7p:value stringValue="201"/&gt;
                        &lt;/L7p:entry&gt;
                    &lt;/L7p:Parameters&gt;
                &lt;/L7p:Encapsulated&gt;
                &lt;L7p:assertionComment&gt;
                    &lt;L7p:Properties mapValue="included"&gt;
                        &lt;L7p:entry&gt;
